    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "android:dev:production": "set APP_ENV=production&& npx react-native bundle --platform android --dev true --reset-cache --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && npx react-native run-android",
    "ios:dev:production": "set APP_ENV=production&& npx react-native bundle --platform ios --dev true --reset-cache --entry-file index.js --bundle-output ios/main.jsbundle && npx react-native run-ios",
    "android:dev:development": "set APP_ENV=development&& npx react-native bundle --platform android --dev true --reset-cache --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && npx react-native run-android",
